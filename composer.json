{
    "name": "greencheap/greencheap",
    "type": "project",
    "description": "The GreenCheap CMS",
    "version": "3.3.0",
    "keywords": [
        "cms",
        "greencheap",
        "symfony",
        "vue",
        "uikit"
    ],
    "homepage": "https://greencheap.net",
    "license": "MIT",
    "authors": [
        {
            "name": "GreenCheap",
            "homepage": "https://greencheap.net",
            "email": "support@greencheap.net"
        }
    ],
    "require": {
        "php": "^8.0",
        "composer/composer": "~1.9",
        "doctrine/annotations": "~1.13",
        "doctrine/dbal": "^3.1",
        "paragonie/random-lib": "~2.0",
        "maximebf/debugbar": "~1.16",
        "monolog/monolog": "~2.2",
        "nikic/php-parser": "~4.10",
        "psr/log": "~1.1",
        "swiftmailer/swiftmailer": "~6.2",
        "symfony/finder": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/routing": "^5.2",
        "symfony/stopwatch": "^5.2",
        "symfony/templating": "^5.2",
        "symfony/translation": "^5.2",
        "twig/twig": "~2.14",
        "php-curl-class/php-curl-class": "~8.9",
        "ext-json": "*",
        "symfony/mime": "^5.2",
        "google/apiclient": "~2.9",
        "symfony/error-handler": "^5.2"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "phpunit/phpunit": "~8.5"
    },
    "autoload": {
        "psr-4": {
            "GreenCheap\\": "app/modules/application/src",
            "GreenCheap\\Installer\\": "app/installer/src",
            "GreenCheap\\System\\": "app/system/src",
            "GreenCheap\\Config\\": "app/modules/config/src",
            "GreenCheap\\Console\\": "app/console/src",
            "GreenCheap\\Migration\\": "app/modules/migration/src"
        }
    },
    "config": {
        "vendor-dir": "app/vendor"
    }
}
