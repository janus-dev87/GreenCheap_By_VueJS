{
    "name": "greencheap/greencheap",
    "type": "project",
    "description": "The GreenCheap CMS",
    "version": "3.0.9",
    "keywords": ["cms", "greencheap", "symfony", "vue", "uikit"],
    "homepage": "https://greencheap.net",
    "license": "MIT",
    "authors": [{
        "name": "GreenCheap",
        "homepage": "https://greencheap.net",
        "email": "support@greencheap.net"
    }],
    "require": {
        "php": "^8.0.1",
        "composer/composer": "^1.10.19",
        "doctrine/annotations": "^1.11.1",
        "doctrine/dbal": "~2.12.1",
        "paragonie/random-lib": "^2.0.1",
        "maximebf/debugbar": "^1.16.3",
        "monolog/monolog": "^2.0.2",
        "nikic/php-parser": "~4.2",
        "psr/log": "~1.1",
        "swiftmailer/swiftmailer": "~6.2",
        "symfony/finder": "~5.1.5",
        "symfony/http-foundation": "~5.2",
        "symfony/routing": "~5.1.5",
        "symfony/stopwatch": "~5.1.5",
        "symfony/templating": "~5.1.5",
        "symfony/translation": "~5.1.5",
        "twig/twig": "^2.12",
        "php-curl-class/php-curl-class": "^8.7",
        "ext-json": "*",
        "symfony/mime": "^5.1",
        "google/apiclient": "~2.8.3",
        "symfony/error-handler": "^5.2"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "phpunit/phpunit": "~8.5.13"
    },
    "autoload": {
        "psr-4": {
            "GreenCheap\\": "app/modules/application/src",
            "GreenCheap\\Installer\\": "app/installer/src",
            "GreenCheap\\System\\": "app/system/src",
            "GreenCheap\\Config\\": "app/modules/config/src",
            "GreenCheap\\Console\\": "app/console/src",
            "GreenCheap\\Migration\\": "app/modules/migration/src"
        }
    },
    "config": {
        "vendor-dir": "app/vendor"
    }
}
